# Docker Environment Configuration
# Copy to .env and customize

# PostgreSQL
POSTGRES_PASSWORD=your_secure_password_here

# API Port (host:container)
API_PORT=8000

# Database Port (for external access)
DB_PORT=5432

# Dashboard Port
DASHBOARD_PORT=8080

# Simulation Mode (true/false)
# IMPORTANT: Set to false only for live trading
SIMULATION_MODE=true
